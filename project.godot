config_version=5

[application]

config/name="Monster Hunter Armor Wardrobe"
config/version="2.0.0"
run/main_scene="res://scenes/main.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="mh_armor_wardrobe"
